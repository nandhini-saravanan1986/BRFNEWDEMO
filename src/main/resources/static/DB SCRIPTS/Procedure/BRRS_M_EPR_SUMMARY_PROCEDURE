 BEGIN

    DELETE FROM BRRS_M_EPR_SUMMARYTABLE WHERE REPORT_DATE = V_REPORT_DATE;

    COMMIT;

-----------------------------------------select row wise

----R11

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_MARKET
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C2';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_GPFSR_NOM_AMT
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C3';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_GPFSR_POS_ATT8_PER_SPE_RIS
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C4';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_GPFSR_NOM_AMT1
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C6';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_GPFSR_POS_ATT4_PER_SPE_RIS
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C7';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_GPFSR_NOM_AMT2
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C9';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_GPFSR_POS_ATT2_PER_SPE_RIS
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C10';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_NET_POS_GEN_MAR_RIS
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C13';


