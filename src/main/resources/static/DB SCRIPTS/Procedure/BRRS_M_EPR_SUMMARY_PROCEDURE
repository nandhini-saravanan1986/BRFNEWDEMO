FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R23' AND COLUMN_ID = 'C10';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R23_GPFSR_CHRG2
FROM  BRRS_M_EPR_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R23' AND COLUMN_ID = 'C11';







--------------------------------------calculations-------row wise-----------------------------------------

--R11


V_R11_GPFSR_CHRG := V_R11_GPFSR_POS_ATT8_PER_SPE_RIS * 0.08;

V_R11_GPFSR_CHRG1 := V_R11_GPFSR_POS_ATT4_PER_SPE_RIS * 0.04;

V_R11_GPFSR_CHRG2 := V_R11_GPFSR_POS_ATT2_PER_SPE_RIS * 0.02;

V_R11_TOT_SPE_RIS_CHRG := V_R11_GPFSR_CHRG +  V_R11_GPFSR_CHRG1 + V_R11_GPFSR_CHRG2;

V_R11_GEN_MAR_RIS_CHRG_8PER :=  V_R11_NET_POS_GEN_MAR_RIS * 0.08;

V_R11_2PER_GEN_MAR_RIS_CHRG_DIV_PORT :=  V_R11_NET_POS_GEN_MAR_RIS * 0.02;

V_R11_TOT_GEN_MAR_RISK_CHRG := V_R11_GEN_MAR_RIS_CHRG_8PER + V_R11_2PER_GEN_MAR_RIS_CHRG_DIV_PORT;

V_R11_TOT_MAR_RIS_CHRG :=  V_R11_TOT_SPE_RIS_CHRG + V_R11_TOT_GEN_MAR_RISK_CHRG;


--R12

V_R12_GPFSR_CHRG := V_R12_GPFSR_POS_ATT8_PER_SPE_RIS * 0.08;

