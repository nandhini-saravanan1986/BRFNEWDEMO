------------CALCULATION-------

-----------MONTH





V_R14_MONTH  := V_R15_MONTH  + V_R16_MONTH  + V_R17_MONTH + V_R18_MONTH + V_R19_MONTH + V_R20_MONTH + V_R21_MONTH + V_R22_MONTH + V_R23_MONTH + V_R24_MONTH + V_R25_MONTH + V_R26_MONTH + V_R27_MONTH;

V_R28_MONTH  := V_R29_MONTH  + V_R30_MONTH  + V_R31_MONTH + V_R32_MONTH + V_R33_MONTH + V_R34_MONTH + V_R35_MONTH + V_R36_MONTH ;


V_R37_MONTH  := V_R38_MONTH  + V_R39_MONTH;


V_R40_MONTH  := V_R41_MONTH  + V_R44_MONTH;

V_R41_MONTH  := V_R42_MONTH  + V_R43_MONTH;

V_R44_MONTH  := V_R45_MONTH  + V_R46_MONTH;

V_R10_MONTH  := V_R11_MONTH  + V_R12_MONTH  + V_R13_MONTH + V_R14_MONTH + V_R28_MONTH + V_R37_MONTH + V_R40_MONTH + V_R47_MONTH;


V_R48_MONTH  := V_R49_MONTH  + V_R53_MONTH  + V_R54_MONTH + V_R55_MONTH;

V_R49_MONTH  := V_R50_MONTH  + V_R51_MONTH + V_R52_MONTH;

V_R56_MONTH  := V_R10_MONTH  - V_R48_MONTH;


V_R57_MONTH  := V_R58_MONTH  + V_R59_MONTH + V_R60_MONTH;

V_R61_MONTH  := V_R56_MONTH  - V_R57_MONTH;

V_R62_MONTH  := V_R63_MONTH  + V_R64_MONTH  + V_R65_MONTH + V_R66_MONTH + V_R67_MONTH + V_R68_MONTH + V_R69_MONTH;




V_R75_MONTH  := V_R76_MONTH  + V_R77_MONTH  + V_R78_MONTH + V_R79_MONTH;

V_R70_MONTH  := (V_R71_MONTH  + V_R72_MONTH  + V_R73_MONTH + V_R74_MONTH + V_R75_MONTH )+ V_R80_MONTH ;

V_R81_MONTH  := V_R62_MONTH  - V_R70_MONTH;


V_R82_MONTH  := V_R61_MONTH  + V_R81_MONTH;

V_R84_MONTH  := V_R82_MONTH  - V_R83_MONTH;


V_R86_MONTH  := V_R84_MONTH  - V_R85_MONTH;

---------YTD





V_R14_YTD  := V_R15_YTD  + V_R16_YTD  + V_R17_YTD + V_R18_YTD + V_R19_YTD + V_R20_YTD + V_R21_YTD + V_R22_YTD + V_R23_YTD + V_R24_YTD + V_R25_YTD + V_R26_YTD + V_R27_YTD;


V_R28_YTD  := V_R29_YTD + V_R30_YTD  + V_R31_YTD + V_R32_YTD + V_R33_YTD + V_R34_YTD + V_R35_YTD + V_R36_YTD ;


V_R37_YTD   := V_R38_YTD  + V_R39_YTD;


V_R40_YTD  := V_R41_YTD   + V_R44_YTD;

V_R41_YTD  := V_R42_YTD  + V_R43_YTD;

V_R44_YTD   := V_R45_YTD   + V_R46_YTD;

V_R10_YTD  := V_R11_YTD  + V_R12_YTD  + V_R13_YTD + V_R14_YTD + V_R28_YTD + V_R37_YTD + V_R40_YTD + V_R47_YTD ;

V_R48_YTD  := V_R49_YTD  + V_R53_YTD  + V_R54_YTD + V_R55_YTD;

V_R49_YTD  := V_R50_YTD  + V_R51_YTD + V_R52_YTD;


V_R56_YTD  := V_R10_YTD  - V_R48_YTD;


V_R57_YTD  := V_R58_YTD  + V_R59_YTD + V_R60_YTD;

V_R61_YTD  := V_R56_YTD - V_R57_YTD;

V_R62_YTD  := V_R63_YTD  + V_R64_YTD  + V_R65_YTD + V_R66_YTD + V_R67_YTD + V_R68_YTD + V_R69_YTD;




V_R75_YTD  := V_R76_YTD + V_R77_YTD  + V_R78_YTD + V_R79_YTD;

V_R70_YTD  := (V_R71_YTD + V_R72_YTD  + V_R73_YTD + V_R74_YTD + V_R75_YTD ) + V_R80_YTD ;

V_R81_YTD  := V_R62_YTD  - V_R70_YTD;


V_R82_YTD  := V_R61_YTD  + V_R81_YTD;

V_R84_YTD  := V_R82_YTD  - V_R83_YTD;


V_R86_YTD := V_R84_YTD - V_R85_YTD;

-------------------------------------------------------

