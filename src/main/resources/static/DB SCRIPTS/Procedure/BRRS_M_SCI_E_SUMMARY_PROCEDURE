----------------------------------------

 BEGIN

    DELETE FROM BRRS_M_SCI_E_SUMMARYTABLE WHERE REPORT_DATE = V_REPORT_DATE;

    COMMIT;

-----------------------------------------------



SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R11_MONTH
FROM BRRS_M_SCI_E_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R11' AND COLUMN_ID = 'C3';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R12_MONTH
FROM BRRS_M_SCI_E_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R12' AND COLUMN_ID = 'C3';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R13_MONTH
FROM BRRS_M_SCI_E_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R13' AND COLUMN_ID = 'C3';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R15_MONTH
FROM BRRS_M_SCI_E_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R15' AND COLUMN_ID = 'C3';

