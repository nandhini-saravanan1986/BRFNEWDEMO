--R19
SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_EXPO_CRM
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C3';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_SPE_PRO_EXPO
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C4';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_AMT_ELIG_SHT_NET
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C5';


SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_ELIG_SUB_APP
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C7';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_NON_COL_GUAR
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C8';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_NON_COL_CRE_DER
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C9';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_COL_ELIG_CASH 
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C10';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_COL_ELIG_TREA_BILLS
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C11';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_COL_ELIG_DEB_SEC
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C12';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_COL_ELIG_EQUI 
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C13';


SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_COL_ELIG_UNIT_TRU 
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C14';


SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R19_CRM_SUB_RWA_RIS_CRM
FROM  BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R19' AND COLUMN_ID = 'C17';



