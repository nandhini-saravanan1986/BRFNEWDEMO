---r20
SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R253_CRM_SUB_APP_RWA_APPL_ORG_COUN
FROM BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R253' AND COLUMN_ID = 'C20';




-------------------------------calculation ------“row-wise”

---r19

--c6
V_R19_TOT_EXPO_NET_SPE :=  V_R19_EXPO_CRM - V_R19_SPE_PRO_EXPO - V_R19_AMT_ELIG_SHT_NET;

--c15
V_R19_CRM_SUB_COL_EXP_COV := V_R19_CRM_SUB_NON_COL_GUAR + V_R19_CRM_SUB_NON_COL_CRE_DER +  V_R19_CRM_SUB_COL_ELIG_CASH  + V_R19_CRM_SUB_COL_ELIG_TREA_BILLS + V_R19_CRM_SUB_COL_ELIG_DEB_SEC +
                                   V_R19_CRM_SUB_COL_ELIG_EQUI  + V_R19_CRM_SUB_COL_ELIG_UNIT_TRU  ;

--c16
V_R19_CRM_SUB_COL_ELIG_EXP_NOT_COV :=  V_R19_CRM_SUB_ELIG_SUB_APP - V_R19_CRM_SUB_COL_EXP_COV;

--c18
V_R19_CRM_SUB_RWA_COV_CRM :=  V_R19_CRM_SUB_COL_EXP_COV * V_R19_CRM_SUB_RWA_RIS_CRM;


--c20
V_R19_CRM_SUB_RWA_NOT_COV_CRM :=  V_R19_CRM_SUB_COL_ELIG_EXP_NOT_COV *( V_R19_CRM_SUB_RWA_ORG_COU /100);

--c24
V_R19_CRM_COMP_COL_EXPO_VAL :=  V_R19_CRM_COMP_COL_ELIG_EXPO_VOL_ADJ - V_R19_CRM_COMP_COL_ELIG_FIN_HAI;

--c25
V_R19_RWA_ELIG_EXPO_NOT_COV_CRM  :=  V_R19_CRM_COMP_COL_EXPO_VAL * (V_R19_CRM_SUB_RWA_ORG_COU/100) ;

--c26

V_R19_RWA_UNSEC_EXPO_CRE_RIS := V_R19_TOT_EXPO_NET_SPE - V_R19_CRM_SUB_ELIG_SUB_APP - V_R19_CRM_COMP_COL_EXPO_ELIG ;

---c27
V_R19_RWA_UNSEC_EXPO := V_R19_RWA_UNSEC_EXPO_CRE_RIS * (V_R19_CRM_SUB_RWA_ORG_COU/100)  ;

--c28 
V_R19_RWA_TOT_RIS_WEI_ASS  := V_R19_CRM_SUB_RWA_COV_CRM +   V_R19_CRM_SUB_RWA_NOT_COV_CRM + V_R19_RWA_ELIG_EXPO_NOT_COV_CRM  + V_R19_RWA_UNSEC_EXPO;



---R20

--c6
V_R20_TOT_EXPO_NET_SPE :=  V_R20_EXPO_CRM - V_R20_SPE_PRO_EXPO - V_R20_AMT_ELIG_SHT_NET;

--c15
V_R20_CRM_SUB_COL_EXP_COV := V_R20_CRM_SUB_NON_COL_GUAR + V_R20_CRM_SUB_NON_COL_CRE_DER +  V_R20_CRM_SUB_COL_ELIG_CASH  + V_R20_CRM_SUB_COL_ELIG_TREA_BILLS + V_R20_CRM_SUB_COL_ELIG_DEB_SEC +
                                   V_R20_CRM_SUB_COL_ELIG_EQUI  + V_R20_CRM_SUB_COL_ELIG_UNIT_TRU;

--c16
V_R20_CRM_SUB_COL_ELIG_EXP_NOT_COV :=  V_R20_CRM_SUB_ELIG_SUB_APP - V_R20_CRM_SUB_COL_EXP_COV;

--c18
V_R20_CRM_SUB_RWA_COV_CRM :=  V_R20_CRM_SUB_COL_EXP_COV * V_R20_CRM_SUB_RWA_RIS_CRM;

--c20
V_R20_CRM_SUB_RWA_NOT_COV_CRM :=  V_R20_CRM_SUB_COL_ELIG_EXP_NOT_COV *( V_R20_CRM_SUB_RWA_ORG_COU/100);

--c24
V_R20_CRM_COMP_COL_EXPO_VAL :=  V_R20_CRM_COMP_COL_ELIG_EXPO_VOL_ADJ - V_R20_CRM_COMP_COL_ELIG_FIN_HAI;

--c25
V_R20_RWA_ELIG_EXPO_NOT_COV_CRM  :=  V_R20_CRM_COMP_COL_EXPO_VAL * (V_R20_CRM_SUB_RWA_ORG_COU/100);

--c26
V_R20_RWA_UNSEC_EXPO_CRE_RIS := V_R20_TOT_EXPO_NET_SPE - V_R20_CRM_SUB_ELIG_SUB_APP - V_R20_CRM_COMP_COL_EXPO_ELIG;

--c27
V_R20_RWA_UNSEC_EXPO := V_R20_RWA_UNSEC_EXPO_CRE_RIS * (V_R20_CRM_SUB_RWA_ORG_COU/100);

--c28 
V_R20_RWA_TOT_RIS_WEI_ASS  := V_R20_CRM_SUB_RWA_COV_CRM   +   V_R20_CRM_SUB_RWA_NOT_COV_CRM + V_R20_RWA_ELIG_EXPO_NOT_COV_CRM  + V_R20_RWA_UNSEC_EXPO;


---R21

--c6
V_R21_TOT_EXPO_NET_SPE :=  V_R21_EXPO_CRM - V_R21_SPE_PRO_EXPO - V_R21_AMT_ELIG_SHT_NET;

--c15
V_R21_CRM_SUB_COL_EXP_COV := V_R21_CRM_SUB_NON_COL_GUAR + V_R21_CRM_SUB_NON_COL_CRE_DER +  V_R21_CRM_SUB_COL_ELIG_CASH  + V_R21_CRM_SUB_COL_ELIG_TREA_BILLS + V_R21_CRM_SUB_COL_ELIG_DEB_SEC +
                                   V_R21_CRM_SUB_COL_ELIG_EQUI  + V_R21_CRM_SUB_COL_ELIG_UNIT_TRU;

--c16
V_R21_CRM_SUB_COL_ELIG_EXP_NOT_COV :=  V_R21_CRM_SUB_ELIG_SUB_APP - V_R21_CRM_SUB_COL_EXP_COV;

--c18
V_R21_CRM_SUB_RWA_COV_CRM :=  V_R21_CRM_SUB_COL_EXP_COV * V_R21_CRM_SUB_RWA_RIS_CRM;

--c20
V_R21_CRM_SUB_RWA_NOT_COV_CRM :=  V_R21_CRM_SUB_COL_ELIG_EXP_NOT_COV * (V_R21_CRM_SUB_RWA_ORG_COU/100);

--c24
