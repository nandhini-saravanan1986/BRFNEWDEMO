FROM BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R247' AND COLUMN_ID = 'C15';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R247_CRM_SUB_APP_RWA_RIS_WEI_CRM
FROM BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R247' AND COLUMN_ID = 'C18';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R247_CRM_SUB_APP_RWA_APPL_ORG_COUN
FROM BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R247' AND COLUMN_ID = 'C20';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R247_CRM_COM_APP_COL_CEA_ELIG_CRM
FROM BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R247' AND COLUMN_ID = 'C22';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R247_CRM_COM_APP_COL_ELIG_CEA_VOL_ADJ
FROM BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R247' AND COLUMN_ID = 'C23';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R247_CRM_COM_APP_COL_ELIG_FIN_HAI
FROM BRRS_M_SRWA_12A_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R247' AND COLUMN_ID = 'C24';

