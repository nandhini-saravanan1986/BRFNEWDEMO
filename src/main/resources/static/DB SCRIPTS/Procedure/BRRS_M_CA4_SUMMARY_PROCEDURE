----------------------------------------

 BEGIN

    DELETE FROM BRRS_M_CA4_SUMMARYTABLE WHERE REPORT_DATE = V_REPORT_DATE;

    COMMIT;

-----------------------------------------------


SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R10_AMT_NAME_OF_SUB1
FROM  BRRS_M_CA4_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R10' AND COLUMN_ID = 'C3';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R10_AMT_NAME_OF_SUB2
FROM  BRRS_M_CA4_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R10' AND COLUMN_ID = 'C4';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R10_AMT_NAME_OF_SUB3
FROM  BRRS_M_CA4_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R10' AND COLUMN_ID = 'C5';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R10_AMT_NAME_OF_SUB4
FROM  BRRS_M_CA4_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R10' AND COLUMN_ID = 'C6';

SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R10_AMT_NAME_OF_SUB5
FROM  BRRS_M_CA4_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R10' AND COLUMN_ID = 'C7';


--TOTAL AMT--
SELECT ROUND(ABS(NVL(SUM(ACCT_BALANCE_IN_PULA)/1000, 0))) INTO V_R10_TOT_AMT
FROM  BRRS_M_CA4_DETAILTABLE WHERE REPORT_DATE = V_REPORT_DATE AND ROW_ID = 'R10' AND COLUMN_ID = 'C8';
